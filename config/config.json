{
  "db": {
    "dbType": "env:DB_DRIVER"
  },
  "meddleware": {
    "static": {
      "priority": 30,
      "module": {
        "name": "express",
        "method": "static",
        "arguments": [
          "path:./static"
        ]
      }
    },
    "assets": {
      "priority": 30,
      "module": {
        "name": "express",
        "method": "static",
        "arguments": [
          "path:./build-static"
        ]
      }
    },
    "routes": {
      "priority": 31,
      "module": {
        "name": "express-enrouten",
        "arguments": [
          {
            "directory": "codepath:./server/routes"
          }
        ]
      }
    },
    "renderPage": {
      "priority": 100,
      "module": {
        "name": "codepath:./server/middleware/renderPage",
        "method": "default"
      }
    }
  }
}
