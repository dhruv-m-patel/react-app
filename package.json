{
  "name": "react-app",
  "version": "1.0.0",
  "description": "React app with SSR support",
  "main": "build/server/index.js",
  "scripts": {
    "migration:create": "db-migrate --config migrations/db-migrate.json create",
    "migration:apply": "db-migrate --config migrations/db-migrate.json up",
    "migration:apply:stage": "db-migrate --config migrations/db-migrate.json -e stage up",
    "migration:apply:prod": "db-migrate --config migrations/db-migrate.json -e prod up",
    "migration:undo": "db-migrate --config migrations/db-migrate.json down",
    "build:server": "babel ./src --out-dir build",
    "build:client": "BABEL_ENV=webpack webpack",
    "build": "npm run build:server && npm run build:client",
    "clean": "rimraf build && rimraf build-static",
    "start": "node ./build/server/index.js",
    "start-dev": "npm run clean && babel-node ./src/server/index.js",
    "start:watch": "npm run clean && npm run build && nodemon",
    "lint:js": "eslint src/**/*.js && eslint src/**/*.jsx",
    "lint:css": "stylelint \"./src/**/*.css\"",
    "lint": "npm run lint:js && npm run lint:css",
    "pretest": "jest --clearCache",
    "test": "jest",
    "ci:test": "jest --updateSnapshot --coverage",
    "storybook": "start-storybook -p 3001",
    "build-storybook": "build-storybook"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/dhruv-m-patel/react-app.git"
  },
  "keywords": [
    "react",
    "ssr",
    "express",
    "webpack4"
  ],
  "author": "Dhruv Patel",
  "bugs": {
    "url": "https://github.com/dhruv-m-patel/react-app/issues"
  },
  "homepage": "https://github.com/dhruv-m-patel/react-app#readme",
  "license": "ISC",
  "dependencies": {
    "@babel/runtime": "^7.8.7",
    "@dhruv-m-patel/express-app": "git+https://github.com/dhruv-m-patel/express-app.git#release/1.0.1",
    "@fortawesome/fontawesome-svg-core": "^1.2.27",
    "@fortawesome/free-brands-svg-icons": "^5.12.1",
    "@fortawesome/free-regular-svg-icons": "^5.12.1",
    "@fortawesome/free-solid-svg-icons": "^5.12.1",
    "@fortawesome/react-fontawesome": "^0.1.9",
    "@loadable/component": "^5.12.0",
    "@loadable/server": "^5.12.0",
    "classnames": "^2.2.6",
    "confit": "^2.3.0",
    "db-migrate": "^0.11.6",
    "db-migrate-mysql": "^2.1.0",
    "db-migrate-pg": "^1.0.0",
    "express-enrouten": "^1.3.0",
    "fast-deep-equal": "^2.0.1",
    "meddleware": "^3.0.3",
    "mysql": "^2.17.1",
    "pg": "^7.18.1",
    "prop-types": "^15.7.2",
    "react": "^16.13.0",
    "react-bootstrap": "^1.0.0-beta.17",
    "react-dom": "^16.13.0",
    "react-helmet": "^5.2.1",
    "react-redux": "^7.2.0",
    "react-router-dom": "^5.1.2",
    "redux": "^4.0.5",
    "redux-api-middleware": "^3.2.0",
    "shortstop-handlers": "^1.0.1",
