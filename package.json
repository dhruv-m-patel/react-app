    "postcss-preset-env": "^6.7.0",
    "react-test-renderer": "^16.13.0",
    "rimraf": "^3.0.2",
    "storybook-react-router": "^1.0.8",
    "style-loader": "^1.1.3",
    "stylelint": "^11.1.1",
    "stylelint-config-recommended": "^3.0.0",
    "terser-webpack-plugin": "^2.3.5",
    "webpack-cli": "^3.3.11",
